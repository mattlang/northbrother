package com.blackhammer.game{		import com.blackhammer.util.*	import flash.events.*;	import flash.utils.Timer;	import flash.display.*;	import flash.media.*;	import flash.net.URLRequest;	import fl.transitions.Tween;	import fl.transitions.easing.*;		public class InfoScreen extends Sprite {				private var _ScreenParent:Object;		private var _background:MovieClip = new info_back();		private var _backButton = new back_button_long(); //why does Button type not work. What is imported?		private var _resetButton = new reset_button();		private var _mouseUpSnd:Sound= new placeObject_snd();				public function InfoScreen(screenParent):void{			_ScreenParent = screenParent;			addChild(_background);			addChild(_backButton);			_backButton.x = 86;			_backButton.y = 445;			_backButton.addEventListener(MouseEvent.MOUSE_UP,onMouseUpButtonEvent, false, 0, true);			addChild(_resetButton);			_resetButton.x = 86;			_resetButton.y = 405;			_resetButton.addEventListener(MouseEvent.MOUSE_UP,onMouseUpResetEvent, false, 0, true);					}				private function onMouseUpButtonEvent(e:Event):void{			_mouseUpSnd.play();						_backButton.removeEventListener(MouseEvent.MOUSE_UP, onMouseUpButtonEvent);			this.RemoveModal();			//_ScreenParent.NavigateScreen("GameScreen");		}				private function onMouseUpResetEvent(e:Event):void{			_mouseUpSnd.play();			_resetButton.removeEventListener(MouseEvent.MOUSE_UP, onMouseUpButtonEvent);			removeChild(_background);			removeChild(_backButton);			_ScreenParent.ResetToLevelOne();			//_ScreenParent.NavigateScreen("GameScreen");		}				public function RemoveModal():void{			removeChild(_background);			removeChild(_backButton);			//trace(_ScreenParent);			_ScreenParent.RemoveInfoModal();					}				public function Kill():void{			removeChild(_background);			removeChild(_backButton);					}			}	}