package com.blackhammer.stereviewer {		import flash.display.MovieClip;	import flash.display.Bitmap;	import flash.display.BitmapData;		import com.greensock.*;	import com.greensock.easing.*;		public class TitleScreen extends MovieClip{				public var _type:String = "open";		private var _isShown:Boolean = false;				public function TitleScreen() {					}				public function mShow():void{			if (_isShown) return;			_isShown = true;			var backBMD:BitmapData = new open_image(0,0); 			var backBM:Bitmap = new Bitmap(backBMD);			var backSepiaBMD:BitmapData = new open_image_sepia(0,0);			var backSepiaBM:Bitmap = new Bitmap(backSepiaBMD);			var topTitle:MovieClip = new Title_1();			var bottomTitle:MovieClip = new Title_2();			backBM.alpha=0;			backSepiaBM.alpha=0;			topTitle.x = -topTitle.width;			topTitle.y = 549;			bottomTitle.x = -bottomTitle.width;			bottomTitle.y = 611;			addChild(backSepiaBM);			addChild(backBM);			addChild(topTitle);			addChild(bottomTitle);			var openTimeline:TimelineMax = new TimelineMax({onComplete:showDone,onCompleteParams:[backSepiaBM]});			openTimeline.append( TweenLite.to(backSepiaBM,2,{alpha:1.0}) );			openTimeline.append( TweenLite.to(topTitle,.7,{delay:0,x:49}) );			openTimeline.append( TweenLite.to(bottomTitle,.7,{delay:1,x:49}) );			openTimeline.append( TweenLite.to(backBM,3,{delay:1.5,alpha:1.0}) );					}		public function mUnload():void{					}		public function mLoad():void{					}		private function showDone(backBM):void{			removeChild(backBM);		}		public function setColors(backColor:uint,fontColor:uint):void{					}		public function kill():void{					}		public function mHide():void{					}	}	}